config_version=5

[application]

config/name="ludum-dare-58"
config/features=PackedStringArray("4.5", "Forward Plus")
config/icon="res://icon.svg"

[autoload]

Utils="*res://src/autoload/Utils.gd"
Signals="*res://src/autoload/Signals.gd"
Global="*res://src/autoload/Global.gd"
DexManager="*res://src/autoload/DexManager.gd"

[display]

window/size/viewport_width=1920
window/size/viewport_height=1080
window/stretch/mode="viewport"

[layer_names]

2d_physics/layer_1="wall"
2d_physics/layer_2="mook"

[rendering]

textures/canvas_textures/default_texture_filter=0
